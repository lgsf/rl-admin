<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 630" width="1200" height="630">
  <!-- Background -->
  <rect width="1200" height="630" fill="#0f172a"/>
  
  <!-- Grid pattern -->
  <defs>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <rect width="40" height="40" fill="none" stroke="#1e293b" stroke-width="0.5"/>
    </pattern>
  </defs>
  <rect width="1200" height="630" fill="url(#grid)" opacity="0.3"/>
  
  <!-- Gradient background -->
  <defs>
    <radialGradient id="glow" cx="50%" cy="50%">
      <stop offset="0%" stop-color="#3b82f6" stop-opacity="0.2"/>
      <stop offset="100%" stop-color="#3b82f6" stop-opacity="0"/>
    </radialGradient>
  </defs>
  <ellipse cx="600" cy="315" rx="400" ry="200" fill="url(#glow)"/>
  
  <!-- Logo container -->
  <rect x="500" y="215" width="200" height="200" rx="20" fill="#1e293b" stroke="#334155" stroke-width="2"/>
  
  <!-- RL Text -->
  <text x="600" y="340" font-family="system-ui, -apple-system, sans-serif" font-weight="bold" font-size="120" fill="#f1f5f9" text-anchor="middle">RL</text>
  
  <!-- Admin text -->
  <text x="600" y="480" font-family="system-ui, -apple-system, sans-serif" font-weight="300" font-size="36" fill="#94a3b8" text-anchor="middle">ADMIN DASHBOARD</text>
</svg>